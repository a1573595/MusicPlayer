<resources>
    <string name="app_name" translatable="false">MusicPlayer</string>

    <!-- LeakCanary
     In version 2.7, the default behavior changed: LeakCanary will continue to detect leaks of toasts,
     but will ignore root views created by a PopupWindow (which is what Android widgets use). -->
    <bool name="leak_canary_watcher_watch_dismissed_dialogs">true</bool>

    <string name="agree">Agree</string>
    <string name="cancel">Cancel</string>
    <string name="disagree">Disagree</string>
    <string name="download">Download</string>
    <string name="download_music">Download music</string>
    <string name="found_new_song">Fond new song</string>
    <string name="need_permission_to_access">Need permission to access music.</string>
    <string name="no_song_found">No song found</string>
    <string name="permission_requirement">Permission requirement</string>
    <string name="search_song">Search song</string>
    <string name="unknown">\u003c unknown \u003e</string>
    <string name="unsupported_format">Unsupported format</string>
    <string name="press_again_to_exit">Press again to exit</string>
    <string name="cant_open_browser">Can\'t open browser.</string>

    <declare-styleable name="PlayPauseState">
        <attr name="state_pause" format="boolean" />
    </declare-styleable>
</resources>
